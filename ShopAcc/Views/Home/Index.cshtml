
@{
    ViewBag.Title = "Index";
    Layout = "~/Views/Shared/_Layout.cshtml";
}


<div class="row">
    <div class="col-sm-12">
        <h1 class="product_taital">Our Products</h1>
    </div>
    @Html.Action("menucategory", "Menu")
</div>
<div class="product_section_2 layout_padding">
    @Html.Action("Partial_Product", "Products")
</div>

<div class="product_classify">
    <div class="container">
        <div class="row">
            <div class="col">
                <div class="Sum_Game">
                    <img style="height:150px;" class="img_sum" src="~/Content/images/44.gif" />
