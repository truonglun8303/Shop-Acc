                    <div class="xem_them">
                        <a href="#">Xem thêm</a>
                    </div>
                    
                </div>
            </div>
            <div class="col">
                <div class="Sum_Game">
                    <img style="height:150px;" class="img_sum" src="~/Content/images/9.gif" />
                    <h3>Acc Free Fire Siêu rẻ</h3>
                    <div class="xem_them">
                        <a href="#">Xem thêm</a>
                    </div>
                </div>
            </div>
            <div class="col">
                <div class="Sum_Game">
                    <img style="height:150px;" class="img_sum" src="~/Content/images/Xa-kho-nick-Lien-Quan.gif" />
                    <h3>Acc Liên Quân Siêu rẻ</h3>
                    <div class="xem_them">
                        <a href="#">Xem thêm</a>
                    </div>
                </div>
            </div>
            <div class="col">
                <div class="Sum_Game">
                    <img style="height:150px;" class="img_sum" src="~/Content/images/MUA-NICK-PUBG-MOBILE.gif" />
                    <h3>Acc Liên Pubg Siêu rẻ</h3>
                    <div class="xem_them">
                        <a href="#">Xem thêm</a>
                    </div>
                </div>
            </div>
        </div>
    </div>
</div>
